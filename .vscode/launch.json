    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'aoc2021-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=aoc2021-rs",
                    "--package=aoc2021-rs"
                ],
                "filter": {
                    "name": "aoc2021-rs",
                    "kind": "bin"
                }
            },
            "args": ["25", "1", "test.txt"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'aoc2021-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=aoc2021-rs",
                    "--package=aoc2021-rs"
                ],
                "filter": {
                    "name": "aoc2021-rs",
                    "kind": "bin"
                }
            },
            "args": ["--", "--nocapture", "test_example_path_neighbours2"],
            "cwd": "${workspaceFolder}"
        }
    ]
}